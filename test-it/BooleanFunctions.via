define (Test dv(.VirtualInstrument (
 c(
    e(dv(.Boolean t) ct)
    e(dv(.Boolean f) cf)
    e(.Boolean x)
    e(.Boolean y)
    e(.Boolean value)
 ) 
  clump(1 
    Copy(cf x)
    Copy(cf y)
    Trigger(1)
    Wait(1)
    
    Copy(ct x)
    Copy(cf y)
    Trigger(1)
    Wait(1)
   
    Copy(cf x)
    Copy(ct y)
    Trigger(1)
    Wait(1)

    Copy(ct x)
    Copy(ct y)
    Trigger(1)
    Wait(1)
    //----------------
   )

    clump(1 
    Printf("-------------\n")
    And(x y value)
    Printf(" And (%s %s) => %s\n" x y value)

    Or(x y value)
    Printf(" Or (%s %s) => %s\n" x y value)

    Nor(x y value)
    Printf(" Nor (%s %s) => %s\n" x y value)

    Nand(x y value)
    Printf(" Nand (%s %s) => %s\n" x y value)

    Xor(x y value)
    Printf(" Xor (%s %s) => %s\n" x y value)

    Not(x value)
    Printf(" Not (%s) => %s\n" x value)

    Not(y value)
    Printf(" Not (%s) => %s\n" y value)
   )
 ) ) )
enqueue (Test)



