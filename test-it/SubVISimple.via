//trace
define (SubVI0 dv(.VirtualInstrument (
    Locals:c( e(dv(.String 'Inside call') sInside) )
    clump(1 
        Println(sInside)
    )
 )))

define (SubVI1  dv(.VirtualInstrument (
    Params:c(i(.Int32 x))
    Locals:c(e(dv(.String 'Inside call') sInside))
    clump(1 
        Println(sInside)
        Println(x)
        )
 )))

define (SubVI2  dv(.VirtualInstrument (
    Params:c(i(.Int32 x) o(.Int32 x_out))
    Locals:c(
        e(dv(.String 'Inside call') sInside)
        e(dv(.Int32 1) c1)
    )
    clump(1 
        Println(sInside)
        Printf("In SubVI2 with in and out parameters initially <%s> <%s>, one is <%s>\n" x x_out c1)
        Add(x c1 x_out)
        Printf("In SubVI2 with in and out parameters after add <%s> <%s>, one is <%s>\n" x x_out c1)
        Println(x)
    )
 )))

define (SubVI3 dv(.VirtualInstrument (
    Params:c(io(.Int32 x))
    Locals:c(
        e(dv(.String 'Inside call') sInside)
        e(dv(.Int32 1) c1)
    )
    clump(1 
        Printf("In SubVI3 with in/out parameter initially <%s>, one is <%s>\n" x c1)
        Add(x c1 x)
        Printf("In SubVI3 with in/out parameter after add <%s>, one is <%s>\n" x c1)
    )
 )))
 
 
define (SubVI1d dv(.VirtualInstrument (
 Params: c(i(.Double x))
 Locals: c(
    e(dv(.String 'Inside call') sInside)
  )
  clump(1 
    Println(sInside)
  Println(x)
   )
 )))

define (SubVI2d dv(.VirtualInstrument (
 c(i(.Double x) o(.Double x_out))
 c(
    e(dv(.String 'Inside call') sInside)
    e(dv(.Double 1) c1)
  )
  clump(1 
    Println(sInside)
    Printf("In SubVI2d with in and out parameters initially <%s> <%s>, one is <%s>\n" x x_out c1)
    Add(x c1 x_out)
    Printf("In SubVI2d with in and out parameters after add <%s> <%s>, one is <%s>\n" x x_out c1)
    Println(x)
   )
 )))

define (SubVI3d dv(.VirtualInstrument (
 c(io(.Double x))
 c(
    e(dv(.String 'Inside call') sInside)
    e(dv(.Double 1) c1)
  )
  clump(1 
    Printf("In SubVI3d with in/out parameter initially <%s>, one is <%s>\n" x c1)
    Add(x c1 x)
    Printf("In SubVI3d with in/out parameter after add <%s>, one is <%s>\n" x c1)
   )
 )))


define (TopVI dv(.VirtualInstrument (
 Locals:c(
   e(dv(.Int32 42) value)
   e(dv(.Double 42.1) dValue)
    e(dv(.String 'Before call') sBefore)
    e(dv(.String 'After call')  sAfter)
  )
    clump(1 
    SubVI0()
    Println(sBefore)
    SubVI1(value)
    Println(value)
    SubVI2(value value)
    Println(value)
    SubVI3(value) // in-out parameter
    Println(value)
    Println(sAfter)
    
    SubVI1d(dValue)
    SubVI2d(dValue dValue)
    SubVI3d(dValue)
   )
 )))

enqueue (TopVI)
