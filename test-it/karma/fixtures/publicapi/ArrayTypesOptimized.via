    // Supported array types
    e(dv(a(.Int8 *) (8 6 7 5 3 0 9 0 -128 127)) arrayInt8)
    e(dv(a(.Int16 *) (8 6 7 5 3 0 9 0 -32768 32767)) arrayInt16)
    e(dv(a(.Int32 *) (8 6 7 5 3 0 9 0 -2147483648 2147483647)) arrayInt32)
