define(MyVI dv(VirtualInstrument (
    Locals: c(
        e('' cookieFile)
        e('' username)
        e('' password)
        e(dv(.UInt32 1) verifyServer)
        e(dv(.UInt32 0) handle)
        e(c(
            e(.Boolean status)
            e(.Int32 code)
            e(.String source)
        ) error)
    )
    clump (
        // Cookie file(0), username(1), password(2), verify Server(3), handle(4), error cluster(5)
        HttpClientOpen(cookieFile username password verifyServer handle error)
    )
) ) )

enqueue(MyVI)
