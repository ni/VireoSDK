define (MyVI dv(.VirtualInstrument (
    Locals: c(
        e(.Boolean booleanLocal)
        e(.String stringLocal)
        e(.Double doubleLocal)
        e(.Int32 int32Local)
        e(.UInt32 uint32Local)
        e(.ComplexDouble complexDoubleLocal)
        e(.Timestamp timestampLocal)

        e(c(
            e(.Boolean status)
            e(.Int32 code)
            e(.String source)
        ) error)
    )
        clump(1
            PropertyNodeRead("MyVI" "dataItem_Boolean" "value" booleanLocal error)
            PropertyNodeRead("MyVI" "dataItem_String" "value" stringLocal error)
            PropertyNodeRead("MyVI" "dataItem_Double" "value" doubleLocal error)
            PropertyNodeRead("MyVI" "dataItem_Int32" "value" int32Local error)
            PropertyNodeRead("MyVI" "dataItem_UInt32" "value" uint32Local error)
            PropertyNodeRead("MyVI" "dataItem_ComplexDouble" "value" complexDoubleLocal error)
            PropertyNodeRead("MyVI" "dataItem_Timestamp" "value" timestampLocal error)
        )
)))
enqueue(MyVI)
