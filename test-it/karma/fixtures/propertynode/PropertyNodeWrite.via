define (MyVI dv(.VirtualInstrument (
    Locals: c(
        e(dv(.Boolean false) myBoolValue)
        e(dv(.String 'Dolor amet sit amet') stringLocal)
        e(dv(.Double 1234.5678) doubleLocal)
        e(dv(.Int32 -1000) int32Local)
        e(dv(.UInt32 9876543) uint32Local)
        e(dv(.ComplexDouble (5.045 -5.67)) complexDoubleLocal)
        e(dv(.Timestamp (3564057536 7811758927381448193)) timestampLocal)

        e(c(
            e(.Boolean status)
            e(.Int32 code)
            e(.String source)
        ) error)
	e(dv(ControlRefNum ControlReference("dataItem_Boolean")) booleanRef)
	e(dv(ControlRefNum ControlReference("dataItem_String")) stringRef)
	e(dv(ControlRefNum ControlReference("dataItem_Double")) doubleRef)
	e(dv(ControlRefNum ControlReference("dataItem_Int32")) int32Ref)
	e(dv(ControlRefNum ControlReference("dataItem_UInt32")) uInt32Ref)
	e(dv(ControlRefNum ControlReference("dataItem_ComplexDouble")) complexDoubleRef)
	e(dv(ControlRefNum ControlReference("dataItem_Timestamp")) timestampRef)
    )
        clump(1
            PropertyNodeWrite(booleanRef "value" myBoolValue error)
            PropertyNodeWrite(stringRef "value" stringLocal error)
            PropertyNodeWrite(doubleRef "value" doubleLocal error)
            PropertyNodeWrite(int32Ref "value" int32Local error)
            PropertyNodeWrite(uInt32Ref "value" uint32Local error)
            PropertyNodeWrite(complexDoubleRef "value" complexDoubleLocal error)
            PropertyNodeWrite(timestampRef "value" timestampLocal error)
        )
)))
enqueue(MyVI)
