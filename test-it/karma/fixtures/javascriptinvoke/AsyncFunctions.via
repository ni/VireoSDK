define(SingleFunction dv(VirtualInstrument (
    Locals: c(
        e(c(
            e(.Boolean status)
            e(.Int32 code)
            e(.String source)
        ) error)
        e(dv(.UInt8 2) input)
        e(dv(.UInt8 0) return)
        e(.Occurrence occurrence)
    )
    clump (
        JavaScriptInvoke(occurrence false error 'SingleFunction' return input)
    )
) ) )

