define(MyVI dv(VirtualInstrument (
    Locals: c(
        e(c(
            e(.Boolean status)
            e(.Int32 code)
            e(.String source)
        ) error)

        e(c(
            e(.Boolean state)
        ) system)
        e(.Occurrence occurrence)
    )
    clump (
        JavaScriptInvoke(occurrence false error 'NI_FunctionWithInvalidParameterType' * system)
    )
) ) )

enqueue(MyVI)
