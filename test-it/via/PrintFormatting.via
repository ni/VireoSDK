define(HelloWorld dv(.VirtualInstrument (
    c(
        e(dv(.String "abc") vStr)
        e(dv(.Int32 427) vInt)
        e(dv(.Double 42.4) vReal)
    )
    clump (1
        Printf("String with default justify   -----------\n")
        Printf("String no field width         -%s-\n" vStr)
        Printf("String to samll field width   -%1s-\n" vStr)
        Printf("String matching field width   -%3s-\n" vStr)
        Printf("String larger   field width   -%4s-\n" vStr)
        Printf("String xlarge   field width   -%50s-\n" vStr)
        Printf("String xlarge   field width   -%51s-\n" vStr)

        Printf("String with left justify set  -----------\n")
        Printf("String no field width         -%-s-\n" vStr)
        Printf("String to samll field width   -%-1s-\n" vStr)
        Printf("String matching field width   -%-3s-\n" vStr)
        Printf("String larger   field width   -%-4s-\n" vStr)
        Printf("String xlarge   field width   -%-50s-\n" vStr)
        Printf("String xlarge   field width   -%-51s-\n" vStr)

        Printf("Int with default justify     -----------\n")
        Printf("Int no field width           -%d-\n" vInt)
        Printf("Int to samll field width     -%1d-\n" vInt)
        Printf("Int matching field width     -%3d-\n" vInt)
        Printf("Int larger   field width     -%4d-\n" vInt)
        Printf("Int xlarge   field width     -%50d-\n" vInt)
        Printf("Int xlarge   field width     -%51d-\n" vInt)

        Printf("Int with left justify set    -----------\n")
        Printf("Int no field width           -%-d-\n" vInt)
        Printf("Int to samll field width     -%-1d-\n" vInt)
        Printf("Int matching field width     -%-3d-\n" vInt)
        Printf("Int larger   field width     -%-4d-\n" vInt)
        Printf("Int xlarge   field width     -%-50d-\n" vInt)
        Printf("Int xlarge   field width     -%-51d-\n" vInt)

        Printf("Real with default justify    -----------\n")
        Printf("Real no field width          -%.0f-\n" vReal)
        Printf("Real to samll field width    -%1.0f-\n" vReal)
        Printf("Real matching field width    -%3.0f-\n" vReal)
        Printf("Real larger   field width    -%4.0f-\n" vReal)
        Printf("Real xlarge   field width    -%50.0f-\n" vReal)
        Printf("Real xlarge   field width    -%51.0f-\n" vReal)

        Printf("Real with left justify set   -----------\n")
        Printf("Real no field width          -%-.0f-\n" vReal)
        Printf("Real to samll field width    -%-1.0f-\n" vReal)
        Printf("Real matching field width    -%-3.0f-\n" vReal)
        Printf("Real larger   field width    -%-4.0f-\n" vReal)
        Printf("Real xlarge   field width    -%-50.0f-\n" vReal)
        Printf("Real xlarge   field width    -%-51.0f-\n" vReal)

        Printf("Real with default justify    -----------\n")
        Printf("Real no field width          -%.1f-\n" vReal)
        Printf("Real to samll field width    -%1.1f-\n" vReal)
        Printf("Real matching field width    -%4.1f-\n" vReal)
        Printf("Real larger   field width    -%5.1f-\n" vReal)
        Printf("Real xlarge   field width    -%50.1f-\n" vReal)
        Printf("Real xlarge   field width    -%51.1f-\n" vReal)

        Printf("Real with left justify set   -----------\n")
        Printf("Real no field width          -%-.1f-\n" vReal)
        Printf("Real to samll field width    -%-1.1f-\n" vReal)
        Printf("Real matching field width    -%-4.1f-\n" vReal)
        Printf("Real larger   field width    -%-5.1f-\n" vReal)
        Printf("Real xlarge   field width    -%-50.1f-\n" vReal)
        Printf("Real xlarge   field width    -%-51.1f-\n" vReal)

    )
) ) )
enqueue(HelloWorld)
