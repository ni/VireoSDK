
define(TestProgram dv(.VirtualInstrument (
    c(
        e(.Queue<.Double> q)
        e(dv(.Double 42.5) x)
        e(.Double y)
        e(.Boolean timedOut)
        e(dv(.Double 1.1) c1_1)
        e(dv(.Double 1.2) c1_2)
        e(dv(.Double 1.3) c1_3)
        e(dv(.Double 1.4) c1_4)
    )

    clump (
        Trigger(1)
        Trigger(2)
        Wait(1)
        Wait(2)
        Printf(" Reader/Writer done\n")
    )

    clump (
        WaitMilliseconds(50)
        EnqueueElement(q c1_1 1000 timedOut)
        Printf(">>Added1 new value %.1f\n" c1_1)

        WaitMilliseconds(51)
        EnqueueElement(q c1_2 1000 timedOut)
        Printf(">>Added1 new value %.1f\n" c1_2)

        WaitMilliseconds(52)
        EnqueueElement(q c1_3 1000 timedOut)
        Printf(">>Added1 new value %.1f\n" c1_3)

        WaitMilliseconds(53)
        EnqueueElement(q c1_4 1000 timedOut)
        Printf(">>Added1 new value %.1f\n" c1_4)
    )

    clump (
        WaitMilliseconds(61)
        DequeueElement(q y 55 timedOut)
        Printf("Dequeued1 value is %.1f to:%s\n" y timedOut)
        DequeueElement(q y 70 timedOut)
        Printf("Dequeued2 value is %.1f to:%s\n" y timedOut)
        DequeueElement(q y 72 timedOut)
        Printf("Dequeued3 value is %.1f to:%s\n" y timedOut)
        DequeueElement(q y 73 timedOut)
        Printf("Dequeued4 value is %.1f to:%s\n" y timedOut)
        DequeueElement(q y 73 timedOut)
        Printf("Dequeued5 value is %.1f to:%s\n" y timedOut)
    )

) ) )


enqueue(TestProgram)
