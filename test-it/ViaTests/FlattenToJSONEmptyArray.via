define(MyVI dv(.VirtualInstrument (
    Locals: c(
        e(dv(a(.Int16 *) () ) arr1)
        e(dv(a(.Int16 * *) () ) arr2)
        e(dv(a(.Int16 * * *) () ) arr3)
        e(dv(a(.Int16 * * * *) () ) arr4)
        e(ErrorCluster err)
        e(dv(.String "defaulttext") result)
    )
    clump(
        FlattenToJSON(arr1 true result err)
        Printf("1: %s %z\n" result err)
        FlattenToJSON(arr2 true result err)
        Printf("2: %s %z\n" result err)
        FlattenToJSON(arr3 true result err)
        Printf("3: %s %z\n" result err)
        FlattenToJSON(arr4 true result err)
        Printf("4: %s %z\n" result err)

        FlattenToJSON(arr1 false result err)
        Printf("5: %s %z\n" result err)
        FlattenToJSON(arr2 false result err)
        Printf("6: %s %z\n" result err)
        FlattenToJSON(arr3 false result err)
        Printf("7: %s %z\n" result err)
        FlattenToJSON(arr4 false result err)
        Printf("8: %s %z\n" result err)
    )
)))
enqueue(MyVI)
