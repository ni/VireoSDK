define(Test dv(.VirtualInstrument (
 c(
    e(.Int32 x)
    e(.Int32 y)
    e(.String output)
    e(dv(.Int32 3) c0)
    e(dv(.Int8) c1)
    e(dv(.Int32 2) c2)
    e(dv(.Single 1) sgl)
    e(dv(.Double 2) dbl)
    e(dv(.Double -2) dbl2)
    e(dv(Enum16 ("abc def" %47hi jkl mno) 2) bar)
    e(dv(Enum32 (red orange yellow green blue purple)) foo)
 )
  clump(1
    //---------------------------------------------------------
    Printf("%d %s\n" bar bar)
    Convert(5 bar)
    Printf("%d %s\n" bar bar)
    Convert(1 bar)
    StringFormat(output "%d" * bar)
    Println(output)
    StringFormat(output "%s" * bar)
    Println(output)
    Convert(c0 bar)
    StringFormat(output "c0 %s" * bar)
    Println(output)
    Convert(c1 bar)
    StringFormat(output "c1 %s" * bar)
    Convert(c2 bar)
    Println(output)
    StringFormat(output "c2 %s" * bar)
    Println(output)
    Convert(sgl bar)
    StringFormat(output "sgl %s" * bar)
    Println(output)
    Convert(dbl bar)
    StringFormat(output "dbl %s" * bar)
    Println(output)
    Convert(dbl2 bar)
    StringFormat(output "dbl2 %s" * bar)
    Println(output)
    Println(foo)
    Add(1 foo foo)
    Println(foo)
    Add(1 foo foo)
    Println(foo)
    Add(1 foo foo)
    Println(foo)
    Add(1 foo foo)
    Println(foo)
    Add(1 foo foo)
    Println(foo)
    Add(1 foo foo)
    //Println(foo)
    Copy(foo foo) // Currently arithmetic ops on an enum require a Copy to itself after to saturate
    Println(foo)
   )
 ) ) )
enqueue(Test)

